    {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [
            {
                // remember to activate the debugger in your workspace config.json
                "name": "Debug visyn server",
                "type": "python",
                "request": "attach",
                "port": 5678,
                "host": "localhost",
                "justMyCode": false,
                "pathMappings": [
                    {
                        "localRoot": "${workspaceFolder}",
                        "remoteRoot": "/phovea"
                    }
                ]
            },
            {
                // remember to start chrome with remote debugging enabled: google-chrome --remote-debugging-port=9222
                "name": "Attach to Chrome",
                "port": 9222,
                "request": "attach",
                "type": "chrome",
                "urlFilter": "http://localhost/*",
                "sourceMapPathOverrides": {
                    "webpack://tdp_publicdb/*": "${workspaceFolder}/*"
                }
          },
          {
                "name": "Launch Chrome",
                "request": "launch",
                "type": "chrome",
                "url": "http://localhost:8080",
                "sourceMapPathOverrides": {
                    "webpack://tdp_publicdb/*": "${workspaceFolder}/*"
                }
          }
        ]
    }
